{
  "name": "Adblockr",
  "version": "1.0.0",
  "description": "A simple chrome adblock extension",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32X32.png",
    "64": "icons/64x64.png",
    "96": "icons/96x96.png",
    "128": "icons/128x128.png",
  },
  "manifest_version": 2
}
